    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.24.1",
    "socket.io": "^4.6.1",
    "stripe": "^14.10.0",
    "twilio": "^4.20.0",
    "winston": "^3.11.0",
